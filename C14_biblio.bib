@article{bronk_ramsey_deposition_2008,
	title = {Deposition models for chronological records},
	volume = {27},
	issn = {0277-3791},
	url = {file://D:\gebruikersgegevens\hanecakr\Mijn documenten\EndNote\Referenties.Data\PDF\BronkRamsey2007-1726750976/BronkRamsey2007.pdf},
	doi = {10.1016/j.quascirev.2007.01.019},
	abstract = {Long-term sedimentary sequences provide a wealth of useful information for research into the palaeo-environment, especially in relation to past climate change. Shorter records provide similar information in many archaeological contexts. However if such records are to be used to provide precise timing of events, and more critically the relative timing between different records, methods are needed to provide accurate and precise age-depth models for these sequences. Given the imprecision of individual calibrated radiocarbon determinations it is necessary to use the information we have about the deposition process to refine our chronologies and also to provide interpolation between dated levels in any sequence. Even with layer counted chronologies, the uncertainties are sometimes hard to quantify. This paper outlines a range of Bayesian models for deposition which have been implemented in the computer programme OxCal. These models can be used to combine information from the sediments themselves with radiocarbon or other direct dating information. Such models have the potential to integrate information between different records and provide a coherent chronology on which to base environmental or archaeological research. (C) 2007 Elsevier Ltd. All rights reserved.},
	number = {1-2},
	journal = {Quaternary Science Reviews},
	author = {Bronk Ramsey, C.},
