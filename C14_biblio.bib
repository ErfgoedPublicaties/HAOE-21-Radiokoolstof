@article{savill_anatomical_1986,
	title = {{ANATOMICAL} {CHARACTERS} {IN} {THE} {WOOD} {OF} {OAK} ({QUERCUS} {ROBUR} {L}. {AND} {Q}.{PETRAEA} {Liebl}.) {WHICH} {PREDISPOSE} {TREES} {TO} {SHAKE}},
	volume = {65},
	issn = {0010-3381},
	url = {http://www.jstor.org/stable/42608066},
	abstract = {The frequent occurrence of shake in oak stands provides a major disincentive to growing the species. The investigation described here was to determine whether there are any anatomical features of the wood which are associated with shaken trees, and which might therefore be used in selection and breeding programmes. Radial wood specimens from the bases of 22 shaken and 19 unshaken oak trees were collected from shake-prone sites. The following observations were then recorded to the nearest 1 μm for each annual growth ring from the 31st to the 60th from the pith: (1) width of one randomly chosen springwood vessel, (2) width of springwood, (3) number of springwood vessels per unit area, (4) width of annual ring, (5) A separate assessment involved the measurement of the width of parenchyma rays on a 3 cm wide tangent across the 45th annual growth ring. An analysis of the results indicated that of these characters only springwood vessel size is significantly correlated with shake: the larger the vessels, the more likely the tree is to be shaken. This character could prove of value in oak breeding programmes if it is heritable, and especially if good juvenile/mature correlations can be established. Work is needed on these aspects. L'existence fréquente de fentes unilatérales dans les peuplements de chênes fournit un découragement important à la culture de l'essence. L'étude décrite ci-après avait comme but de déterminer s'il y a des caractéristiques anatomiques du bois qui sont liées aux arbres avec fentes, et qui pourraient donc être utilisées dans les programmes de sélection et d'amélioration génétique. Des échantillions de bois radial issus des tiges de 22 chênes avec fentes et de 19 chênes sans fentes ont été recueillis de sites prédisposés à fentes. Les observations suivantes ont alors été enregistrées jusqu'au 1 ₀m le plus près pour chaque cerne annuel d'accroissement du 31e jusqu'au 60e de la moelle: (1) la largeur d'un vaisseau de bois initial choisi au hasard, (2) la largeur du bois initial, (3) le nombre de vaisseaux de bois initial par unité de surface, (4) la largeur du cerne annuel, (5) une évaluation indépendante entraînait la mesure de la largeur des rayons de parenchyme sur une tangente de 3 cm de largeur en travers le 45e cerne annuel d'accroissement. Une analyse des résultats montrait que, de ces caractéristiques, seulement la grandeur des vaisseaux de bois initial est en corrélation significative avec les fentes: plus les vaisseaux sont grands, le plus probable il est que l'arbre contient des fentes. Cette cractéristique peut être utile dans les programmes d'amélioration génétique du chêne si elle est héritable, et surtout si de bonnes corrélations juvénile/âgée peuvent être établies. Ces aspects méritent des études plus poussés. La frecuente ocurrencia de rajaduras en los rodales de encino es una importante desmotivación para la plantación de estas especies. La investigación descrita aqui se realizó para determinar si existen rasgos anatómicos de la madera que estan asociados con la rajadura de los arboles, y por lo tanto podrian ser usados en programas de seleccion y mejoramiento. Se colectaron ejemplares de madera radial de la base de 22 arboles de encino con rajadura y 19 sin rajadura en sitios susceptibles a rajaduras. Las siguientes observaciones se registraron al micron más cercano para cada anillo anual de crecimiento de el 30-60 a partir del centro, (i) ancho de un vaso de la madera temprana escogido aleatoriamente, (ii) ancho de la madera temprana, (iii) número de vasos en la madera temprana por unidad de ária, (iv) ancho de anillo anual, (v) una evaluación separada involucró la medición del ancho de los rayos del parenquima en una tangente diagonal de 3 cm. de ancho del 45[symbol] anillo anval de crecimiento. El análisis de los resultados indicó qui de estas caracteristicas únicamente el tamaño de los vasos en la madera temprana está significatiuamente correlacionada con las rajaduras: entre más largos son los vasos, es más probable que los arboles presenten rajaduras. Estos caracteres podrian ser valiosos en programas de mejoramiento de encino si fueran heredables y especialmente si se pueden establecer correlaciones juvenil/maduro. Se requiere más trabajo en estos aspectos.},
	number = {2 (203)},
	urldate = {2017-05-03},
	journal = {The Commonwealth Forestry Review},
	author = {Savill, P. S.},
	year = {1986},
	pages = {109--116}
}

