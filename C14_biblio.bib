	month = aug,
	year = {2007},
	note = {WOS:000247766700008},
	keywords = {dendrochronology, canopy disturbance, forest reserve, forest conversion, recruitment, pinus sylvestris, quercus robur, dead wood, growth, pine, history, stands, regeneration, biodiversity, management, europe},
	pages = {219--228},
