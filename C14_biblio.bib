	note = {WOS:000395862000004},
	keywords = {vegetation, growth, history, oak, Trees, plants, Stools, Spatial Patterns, Root System, Terrestrial Laser Scanning, Ancient Coppices, ecological field-theory, forest, statistics, rings},
	pages = {23--31},
	file = {Vrska_etal2017_iForest.pdf:C\:\\Users\\hanecakr\\Documents\\Zotero\\storage\\32USKTMX\\Vrska_etal2017_iForest.pdf:application/pdf}
