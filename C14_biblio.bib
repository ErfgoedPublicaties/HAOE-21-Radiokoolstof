	pages = {81--90},
	file = {Esper e.a. - 2018 - Large-scale, millennial-length temperature reconst.pdf:C\:\\Users\\hanecakr\\Documents\\Zotero\\storage\\M83H4AZF\\Esper e.a. - 2018 - Large-scale, millennial-length temperature reconst.pdf:application/pdf}
