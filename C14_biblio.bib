	month = aug,
	year = {2018},
	keywords = {Dendrochronology, Czech Republic, Early Neolithic, Linear Pottery Culture (LBK), Oak, Water well},
	pages = {98--104},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\hanecakr\\Documents\\Zotero\\storage\\Z5B89UZD\\Rybníček et al. - 2018 - New dendroarchaeological evidence of water well co.pdf:application/pdf}
