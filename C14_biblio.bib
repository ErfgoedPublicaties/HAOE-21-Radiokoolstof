@mastersthesis{dierickx_niet-destructief_2018,
	address = {Antwerpen},
	title = {Niet-destructief dendrochronologisch onderzoek via x-straal microtomografie},
	abstract = {The goal of dendrochronological analysis of a wooden object is to date the piece of wood in time, by comparing the unique sequence of the measured widths of growth rings to reference chronologies. Conventional methods require a clear surface of the transversal plane of the wood, meaning that the genuine object must be damaged. Dating wood with x-ray computer tomography is an alternative, non-destructive method that has already been proven successful on archaeological objects. This paper strives to establish an overview of the resolution necessary for accurate dendrochronological analysis based on x-ray CT scans. By analysing the scans of drilled core samples of 16 different indigenous wood species at different resolutions, a minimum resolution for each species was concluded. To explore the possibilities of scanning art objects, four oak objects were scanned and analysed at different resolutions. Considering the different sizes and possible paint finishes on the scanned objects, several conclusions about the accuracy, minimum resolution and benefits of x-ray CT scanning for dendrochronological purposes could be drawn.},
	language = {nl},
	school = {Universiteit Antwerpen},
	author = {Dierickx, Sofie},
	year = {2018},
	file = {Dierickx en Fraiture - Niet-destructief dendrochronologisch onderzoek via.pdf:C\:\\Users\\hanecakr\\Documents\\Zotero\\storage\\DF7TNA97\\Dierickx en Fraiture - Niet-destructief dendrochronologisch onderzoek via.pdf:application/pdf}
}

