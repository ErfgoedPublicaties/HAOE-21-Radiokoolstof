	year = {2019},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/jqs.3115},
	keywords = {dendrochronology, Quercus, oak, oxygen, archaeometry, geochronology, isotope},
	pages = {475--490},
	file = {Full Text PDF:C\:\\Users\\hanecakr\\Documents\\Zotero\\storage\\7PWGWIUI\\Loader e.a. - 2019 - Tree ring dating using oxygen isotopes a master c.pdf:application/pdf;Snapshot:C\:\\Users\\hanecakr\\Documents\\Zotero\\storage\\QGG48G5N\\jqs.html:text/html}
}

