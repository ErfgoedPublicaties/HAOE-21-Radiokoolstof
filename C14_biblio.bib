	keywords = {Dendroarchaeology, Blue Intensity, Conifers, Tree-ring dating},
	pages = {99--111},
	file = {Wilson e.a. - 2017 - Facilitating tree-ring dating of historic conifer .pdf:C\:\\Users\\hanecakr\\Documents\\Zotero\\storage\\NUBMD3KA\\Wilson e.a. - 2017 - Facilitating tree-ring dating of historic conifer .pdf:application/pdf}
