	pages = {337--360},
	file = {Ramsey 2009 - Radiocarbon.pdf:C\:\\Users\\hanecakr\\Documents\\Zotero\\storage\\PJ4BX4XT\\Ramsey 2009 - Radiocarbon.pdf:application/pdf}
