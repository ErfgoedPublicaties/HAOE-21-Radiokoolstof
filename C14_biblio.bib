@article{imamura_radiocarbon_2007,
	title = {Radiocarbon wiggle-matching of {Japanese} historical materials with a possible systematic age offset},
	volume = {49},
	issn = {0033-8222},
	url = {file://D:\gebruikersgegevens\hanecakr\Mijn documenten\EndNote\Referenties.Data\PDF\Imamura et al 2007 - Radiocarbon-0524247040/Imamura et al 2007 - Radiocarbon.pdf},
	abstract = {Progress in radiocarbon accelerator mass spectrometry (AMS) techniques enables much more access to wiggle-matching techniques for high-precision C-14 dating with relatively low costs than before. Recently, we have applied wiggle-matching for a number of wood samples where dendrochronology is difficult because of various limitations imposed for dendro-dating, In most cases, wiggle-matching gave rather unambiguous calendar ages, but we found that in some cases the calibrated date was very sensitive to a systematic error of the C-14 date. Here, we present a wooden artifact from the Ujishigai archaeological site as a case where the highest wiggle-matched date did not agree with the date given by dendrochronology. An age with lower probability agreed with the tree-ring age of AD 389, which marked the beginning of the production of Sue ware (unglazed stoneware) in Japan. We show that systematic errors must be carefully taken into account while interpreting C-14 wiggle-matching results, whether they are due to instrumental errors (statistical) or due to a regional offset from the IntCal04 (Reimer et al. 2004) calibration curve.},
	number = {2},
	journal = {Radiocarbon},
	author = {Imamura, M. and Ozaki, H. and Mitsutani, T. and Niu, E. and Itoh, S.},
	year = {2007},
	pages = {331--337},
	file = {Imamura et al 2007 - Radiocarbon.pdf:C\:\\Users\\hanecakr\\Documents\\Zotero\\storage\\29QHFD5C\\Imamura et al 2007 - Radiocarbon.pdf:application/pdf}
}

