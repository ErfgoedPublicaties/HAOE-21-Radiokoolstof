@article{coulthard_limits_2020,
	title = {The limits of freely-available tree-ring chronologies},
	volume = {234},
	issn = {0277-3791},
	url = {https://ui.adsabs.harvard.edu/abs/2020QSRv..23406264C/abstract},
	doi = {10.1016/j.quascirev.2020.106264},
	abstract = {Trees rank as some of the longest-lived organisms on our planet. Because their growth is affected by both biotic and abiotic factors, many species that form annual rings provide an exceptional, long-term archive of past environmental and ecological change. As testament to the deeply-rooted culture of data-sharing by dendrochronologists, tree-ring records are now freely available from thousands of locations worldwide and used extensively in climate science and forest ecology. Many of these datasets are presented in the form of a tree-ring 'chronology' - a composite series made by averaging together measurements of tree-ring width, wood density, isotopic composition, or other anatomical or biogeochemical variables from several dozen or more trees at the same location (Cook and Kairiukstis, 1990). Low-cost environmental datasets of such rich time resolution and spatial coverage are understandably attractive, but using tree-ring chronologies requires a deep understanding of the methods applied to produce them and the limits those methods impose. In this comment, we discuss several important but often unrecognized aspects of tree-ring chronologies that are needed for their interpretation, and highlight ways to make these data more suitable for widespread use.},
	language = {en},
	urldate = {2020-06-23},
	journal = {QSRv},
	author = {Coulthard, Bethany L. and St. George, Scott and Meko, David M.},
	month = apr,
	year = {2020},
	pages = {106264},
	file = {Snapshot:C\:\\Users\\hanecakr\\Documents\\Zotero\\storage\\MTZEA849\\abstract.html:text/html}
}

